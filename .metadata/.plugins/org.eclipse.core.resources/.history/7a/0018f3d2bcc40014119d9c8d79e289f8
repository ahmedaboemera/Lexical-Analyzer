#include "Graph.h"


Graph::Graph() {
	this->count = 0;
	this->root = nullptr;
	this->graphs_nodes_iterator = this->graph_nodes.begin();
}

Node* Graph::add_node() {
	Node* new_node = new Node (to_string(this->count));
	this->graph_nodes.insert(graphs_nodes_iterator, new_node);
	this->count++;
	return new_node
}

Graph::~Graph() {
	// TODO Auto-generated destructor stub
}

