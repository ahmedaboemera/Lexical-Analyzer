#include "Graph.h"

using namespace std;

Graph::Graph() {
	this->count = 0;
	this->root = nullptr;
}

void Graph::add_node(Node* new_node) {
	if (this->count == 0) {
		// first insertion in graph
		string s = std::to_string(this->count);
		Node* new_node = new Node (s);
	}
}

Graph::~Graph() {
	// TODO Auto-generated destructor stub
}

